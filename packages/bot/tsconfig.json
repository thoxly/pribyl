{
  "compilerOptions": {
    "target": "ES2020",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "rootDir": "../",
    "typeRoots": ["./node_modules/@types", "./src/types"],
     "baseUrl": ".",
      "paths": {
      "api/*": ["../api/*"]
    },
  },
  "include": ["src"]
}